{
  "name": "chat-example",
  "version": "0.0.0",
  "description": "A chat example to showcase how to use `socket.io` with a static `express` server with `async` for control flow.",
  "main": "index.js",
  "author": "Jin Ser <jin@ser.sgm>",
  "dependencies": {
    "async": "~0.2.8",
    "body-parser": "^1.18.3",
    "elliptic": "^6.4.1",
    "express": "^4.16.3",
    "hex-to-binary": "^1.0.1",
    "jest": "^23.6.0",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "socket.io": "~0.9.14"
  },
  "scripts": {
    "start": "node index.js",
    "test": "jest --watchAll",
    "dev": "npm run start-redis && nodemon index.js",
    "dev-peer": "cross-env GENERATE_PEER_PORT='true' nodemon index.js",
    "start-redis": "redis-server --daemonize yes"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      ".c9"
    ]
  },
